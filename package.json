  "stylelint": {
    "rules": {
      "indentation": "tab"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "src/tests/**/*.js",
      "src/client/**/*.spec.js"
    ],
    "cache": false
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>__tests__/setup/test-bundler.js",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>__tests__/setup/setupEnzyme.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ]
  }
