  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "vendor": "webpack --config src/webpack/vendor.config.js",
    "start": "node src/index.js",
    "local": "NODE_ENV=development node src/index.js --hot",
    "test": "NODE_ENV=test MOCHA_FILE=./src/test-results/mocha/test-results.xml nyc mocha --opts src/tests/mocha.opts --reporter mocha-junit-reporter",
    "test:local": "NODE_ENV=test mocha --opts src/tests/mocha.opts",
    "test:snapshot": "SNAPSHOT_UPDATE=1 mocha --opts src/tests/mocha.opts",
    "test:watch": "NODE_ENV=test mocha --opts src/tests/mocha.opts --watch",
    "test:coverage": "NODE_ENV=test nyc report",
    "build:qa": "npm rebuild node-sass && NODE_ENV=production webpack --config src/webpack/qa.config.js --colors --display-error-details",
    "build:staging": "npm rebuild node-sass && NODE_ENV=production webpack --config src/webpack/staging.config.js --colors --display-error-details",
    "build:prod": "npm rebuild node-sass && NODE_ENV=production webpack --config src/webpack/prod.config.js --colors --display-error-details",
    "prod": "NODE_ENV=production node src/index.js",
    "release": "standard-version",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "stats": "webpack-bundle-analyzer src/build/stats.json"
  },
